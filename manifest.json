{
  "name" : "YouTube Stream Page",
  "version" : "1.0.4",
  "description" : "Making a better live stream watch page for YouTube",
  "action" : {},
  "icons": {
    "16": "img/logo16.png",
    "32": "img/logo32.png",
    "48": "img/logo48.png",
    "128": "img/logo128.png"
  },
  "manifest_version" : 3,
  "content_scripts": [
    {
      "matches" : ["https://*.youtube.com/*"],
      "js": ["js/main.js"]
    }
  ],
  "host_permissions": ["https://*.youtube.com/*"],
  "web_accessible_resources": [
  {
    "resources": [
      "css/watchpage.css",
      "img/ysp_*.png",
      "js/watchpage.js"
    ],
    "matches": [ "https://*.youtube.com/*" ]
  }]
}